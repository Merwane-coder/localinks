# --- Database ---
DATABASE_URL="postgresql://DB_USER:DB_PASSWORD@DB_HOST:5432/DB_NAME"
DIRECT_URL="postgresql://DB_USER:DB_PASSWORD@DB_HOST:5432/DB_NAME"

# --- Supabase (placeholders sûrs) ---
SUPABASE_URL="YOUR_SUPABASE_URL"
SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"

# --- Auth Google / NextAuth ---
GOOGLE_CLIENT_ID="YOUR_GOOGLE_OAUTH_CLIENT_ID"
GOOGLE_CLIENT_SECRET="YOUR_GOOGLE_OAUTH_CLIENT_SECRET"
GOOGLE_REDIRECT_URI="http://localhost:3000/api/auth/callback/google"
NEXTAUTH_SECRET="YOUR_LONG_RANDOM_STRING"
NEXTAUTH_URL="http://localhost:3000"

# --- Stripe (NE PAS utiliser pk_test / sk_test réels ici) ---
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="YOUR_STRIPE_PUBLISHABLE_KEY"
STRIPE_SECRET_KEY="YOUR_STRIPE_SECRET_KEY"
NEXT_PUBLIC_STRIPE_PRICE_ID_PRO="PRICE_ID_PRO"
NEXT_PUBLIC_STRIPE_PRICE_ID_PREMIUM="PRICE_ID_PREMIUM"
STRIPE_PRICE_ID_PRO="PRICE_ID_PRO"
STRIPE_PRICE_ID_PREMIUM="PRICE_ID_PREMIUM"
STRIPE_WEBHOOK_SECRET="YOUR_STRIPE_WEBHOOK_SECRET"

# --- SMTP ---
SMTP_HOST="smtp.example.com"
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER="SMTP_USER"
SMTP_PASS="SMTP_PASSWORD"
SMTP_FROM="noreply@example.com"

# --- Google Maps ---
NEXT_PUBLIC_GOOGLE_API_KEY="YOUR_GOOGLE_MAPS_API_KEY"

# --- Redis ---
REDIS_DATABASE_URI="redis://localhost:6379/0"
DISABLE_REDIS=1
